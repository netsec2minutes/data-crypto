{
  "name": "data-crypto",
  "version": "1.4.4",
  "description": "encryption algorithms",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "tsc",
    "lint": "eslint --ext '.ts' ./src",
    "test": "yarn prepare && jest",
    "changeLog": "standard-version",
    "prettierAll": "prettier --write \"**/*.ts\""
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn test"
    }
  },
  "keywords": [
    "Des",
    "pin block",
    "data-crypto",
    "cipherjs",
    "cipher",
    "ISO 9564",
    "ANSI X9.8",
    "VISA-1",
    "decrypt",
    "encrypt",
    "crypto",
    "javascript",
    "hossein mohammadi"
  ],
  "author": "Hossein mohammadi (hosseinm.developer@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hosseinmd/data-crypto/issues"
  },
  "homepage": "https://github.com/hosseinmd/data-crypto#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hosseinmd/data-crypto.git"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^12.0.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@typescript-eslint/parser": "^4.17.0",
    "commitlint": "^12.0.1",
    "eslint": "^7.21.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prettier-plugin-jsdoc": "^0.3.13",
    "standard-version": "^9.1.1",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@types/jest": "^26.0.24",
    "buffer": "^6.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/__tests__/**/*.test.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "engines": {
    "node": ">=12.0.0"
  }
}